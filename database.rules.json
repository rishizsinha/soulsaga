{
  "rules": {
    "dataConsent": {
      "$uid": {
        ".read": "$uid == auth.uid",
        ".write": "$uid == auth.uid"
      }
    },
    "timeline": {
      "$uid": {
        ".read": "$uid == auth.uid",
        ".write": "$uid == auth.uid",
        ".indexOn": ["ms"],
      }
    },
    ".read": false,
    ".write": false
  }
}
